<%= link_to "Create User", new_admin_user_path %>
<table>
  <tr>
    <th>User name</th>
    <th>Email</th>
    <th>Created on</th>
    <th>Updated on</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= link_to user.full_name, user_path(user) %></td>
    <td><%= user.email %></td>
    <td><%= formatted_date(user.created_at) %></td>
    <td><%= formatted_date(user.updated_at) %></td>
    <td><%= link_to "Edit", edit_admin_user_path(user) %></td>
    <td><%= link_to "Delete", admin_users_path, data: { confirm: "You sure?" } %></td>
  </tr>
  <% end %>
</table>
<%= paginate @users %>